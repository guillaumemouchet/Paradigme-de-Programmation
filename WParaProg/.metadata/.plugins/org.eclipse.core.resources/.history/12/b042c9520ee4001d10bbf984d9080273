package ch.hearc.Exemples.producteurs_consommateurs.src.Exemple4;

public class Message {
    private String msg;
    private boolean new_value;

    public Message(String str){
        this.msg=str;
        new_value = false;
    }

    public String getMsg() {
        return msg;
    }

    public void setMsg(String str) {
        this.msg=str;
    }

}